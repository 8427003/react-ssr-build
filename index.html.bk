<html><head>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type">
    <title>flv.js demo (v1.4.0)</title>
    <link rel="stylesheet" type="text/css" href="demo.css">
</head>

<body>
    <div class="mainContainer">
        <div class="video-container">
            <div>
                <video name="videoElement" class="centeredVideo" controls="" autoplay="" src="blob:http://bilibili.github.io/23cf7744-e0da-47e2-81dd-2419af1d60c4">
                    Your browser is too old which doesn't support HTML5 video.
                </video>
            </div>
        </div>

    <script src="http://bilibili.github.io/flv.js/dist/flv.js"></script>
    <script>
        var element = document.getElementsByName('videoElement')[0];
        if (typeof player !== "undefined") {
            if (player != null) {
                player.unload();
                player.detachMediaElement();
                player.destroy();
                player = null;
            }
        }
        player = flvjs.createPlayer({
            cors: true,
            hasAudio: true,
            hasVideo: false,
            isLive: true,
            type: "flv",
            url: "http://182.92.3.84:8000/live/flv/GEN-ENC-0/starlight/mlinkm/144114411441",
            withCredentials: false,
            enableWorker: false,
            lazyLoadMaxDuration: 3 * 60,
            seekType: 'range',
        });
        player.attachMediaElement(element);
        player.load();
        player.play();
    </script>
</body></html>
